Package: digitalDLSorteR
Type: Package
Title: Deconvolution of bulk RNA-seq samples based on Deep Neural Networks
Version: 0.1.0
Authors@R: c(
  person(given = "Diego", family = "Ma√±anes", email = "dmananesc@cnic.es", role = c("aut", "cre"), comment = c(ORCID = "0000-0001-7247-6794")),
